{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "ci": "bun run lint && bun run format:check",
    "inngest": "npx inngest-cli@latest dev -u http://localhost:4000/api/inngest"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.2",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clerk/express": "^1.7.56",
    "@e2b/code-interpreter": "^2.3.3",
    "@inngest/agent-kit": "^0.13.2",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "inngest": "^3.47.0",
    "pg": "^8.16.3",
    "random-word-slugs": "^0.1.7",
    "rate-limiter-flexible": "^9.0.1",
    "zod": "^4.1.13"
  }
}
