generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User{
  id Int @id @default(autoincrement())
  name String
  clerkId String @unique
  email String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
